# The name of your C compiler:
CC= cc
CFLAGS= -fPIC -O3
LIB=./lib
INCLUDE = ./include
SRC=./src
OBJ=./obj
BIN=./bin

libtiff: $(LIB)/libtiff.a
	echo "libtiff.a built..."





$(LIB)/libtiff.a: \
$(OBJ)/mkg3states.o \
$(OBJ)/tif_aux.o \
$(OBJ)/tif_close.o \
$(OBJ)/tif_codec.o \
